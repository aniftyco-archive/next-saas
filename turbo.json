{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/master",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "example#start": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"],
      "cache": false
    }
  }
}
